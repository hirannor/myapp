<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://myapp/notification/service/bean"
	xmlns="http://myapp/notification/service/bean" xmlns:customer="http://myapp/notification/service/bean"
	elementFormDefault="qualified">

	<xsd:complexType name="notificationSendRequest">
		<xsd:sequence>
			<xsd:element name="Subject" type="xsd:string"/>
			<xsd:element name="ToEmail" type="xsd:string"/>
			<xsd:element name="Message" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="notificationSendResponse">
		<xsd:sequence>
			<xsd:element name="Message" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
</xsd:schema>